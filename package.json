{
  "name": "mora-scripts",
  "version": "1.2.0",
  "description": "Some scripts for my own use",
  "scripts": {
    "postinstall": "node hooks/scripts/install",
    "postuninstall": "node hooks/scripts/install --reverse",

    "test": "npm run test:run:base && npm run test:run:prefix",
    "test:run:base": "cli/run.js ENV='run test' node test/test-cli-run-base -v pkg.version --bar pkg.args.bar",
    "test:run:prefix": "cli/run.js --prefix % node test/test-cli-run-prefix -v %.version --foo '** %.args.foo **'"
  },
  "args": {
    "foo": "I am foo",
    "bar": "args.foo: pkg.args.foo"
  },
  "bin": {
    "hooks": "hooks/scripts/install.js",
    "run": "cli/run.js"
  },
  "keywords": [],
  "author": "Mora <qiuzhongleiabc@126.com>",
  "license": "MIT",
  "dependencies": {
    "cross-spawn": "^4.0.0",
    "semver-regex": "1.0.0"
  },
  "config": {
    "hooks": {
      "commit-msg": {
        "command": "hooks/commit-msg.js",
        "showHelp": false
      },
      "post-merge": "hooks/post-merge.js"
    }
  }
}
