{
  "name": "mora-scripts",
  "version": "1.1.0",
  "description": "Some scripts for my own use",
  "scripts": {
    "postinstall": "node hooks/scripts/install",
    "postuninstall": "node hooks/scripts/install --reverse"
  },
  "bin": {
    "hooks": "hooks/scripts/install.js"
  },
  "keywords": [],
  "author": "Mora <qiuzhongleiabc@126.com>",
  "license": "MIT",
  "dependencies": {
    "cross-spawn": "^4.0.0",
    "semver-regex": "1.0.0"
  },
  "config": {
    "hooks": {
      "commit-msg": {
        "command": "hooks/commit-msg.js",
        "showHelp": false
      },
      "post-merge": "hooks/post-merge.js"
    }
  }
}
